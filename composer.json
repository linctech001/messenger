{
    "name": "linctech/messenger",
    "description": "Laravel messenger suite, forked and customized by Linctech.",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Tippin",
            "email": "tippindev@gmail.com",
            "homepage": "https://tippindev.com"
        }
    ],
    "homepage": "https://github.com/linctech001/messenger",
    "keywords": [
        "Laravel",
        "Messenger",
        "Linctech"
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "ext-json": "*",
        "intervention/image": "^2.5",
        "intervention/imagecache": "^2.5",
        "joypixels/emoji-toolkit": "^6.0",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Linctech\\Messenger\\": "src/",
            "Linctech\\Messenger\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linctech\\Messenger\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Linctech\\Messenger\\MessengerServiceProvider"
            ],
            "aliases": {
                "Messenger": "Linctech\\Messenger\\Facades\\Messenger"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
